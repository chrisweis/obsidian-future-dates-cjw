{
	"name": "obsidian-future-dates-cjw",
	"version": "1.6.0",
	"description": "Show list of future dates in vault with enhanced display (Fork)",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"deploy": "npm run build && node deploy.mjs",
		"deploy:only": "node deploy.mjs",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": ["obsidian", "obsidian-plugin", "future-dates", "planning", "daily-notes"],
	"author": "Chris Weis (fork of original by Dmitry Manannikov)",
	"license": "The Unlicense",
	"repository": {
		"type": "git",
		"url": "https://github.com/chrisweis/obsidian-future-dates-cjw"
	},
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"obsidian-daily-notes-interface": "^0.9.4"
	}
}
